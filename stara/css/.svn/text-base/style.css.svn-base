@font-face {
	font-family: '/font/ArmWrestlerBold';
	src: url('/font/ArmWrestler-webfont.eot');
	src: url('/font/ArmWrestler-webfont.eot?#iefix') format('embedded-opentype'),
	url('/font/ArmWrestler-webfont.woff') format('woff'),
	url('/font/ArmWrestler-webfont.ttf') format('truetype'),
	url('/font/ArmWrestler-webfont.svg#ArmWrestlerBold') format('svg');
	font-weight: normal;
	font-style: normal;

}

body {
	background: url(/img/background.png);
	padding: 35px 0;
	font-family: 'Archivo Narrow', sans-serif;
}

.wrapper {
	width: 960px;
	margin: 0 auto;
}

header {
	margin-bottom: 30px;
}
header h1 {
	width: 529px;
	height: 79px;
	margin: 0 auto;
	background: url(/img/title.png) no-repeat 0 0;
}
header h1 a {
	display: block;
	width: 529px;
	height: 79px;
	text-indent: -1000em;
	overflow: hidden;
}
h2 {
	margin: 0 0 20px;
}
hr {
	clear: both;
	background: url(/img/hr.png) no-repeat;
	height: 1px;
	border: 0;
	margin: 0 0 20px;
}

.botext {
	display: none;
}
.bottom_text {
	padding: 20px 40px 40px;

}
h1, h2, h3, h4 {
	font-family: 'Abril Fatface', cursive;
	text-transform: uppercase;
	margin-top: 0 !important;
}
.toplist,
.bottomlist {
}
.toplist {
	padding: 45px 50px 60px;
	background: url(/img/list_top.png) no-repeat 0 bottom;
}
.bottomlist {
	position: relative;
	top: -35px;
	background: url(/img/list_bottom_sh.png) no-repeat 0 bottom;
}
.bottomlist div {
	background: url(/img/list_bot.png) no-repeat 0 top;
	padding: 45px 50px 33px;
	margin-bottom: 26px;
}
.toplist a,
.bottomlist a {
	float: right;
	background: #1d7373;
	padding: 12px 20px;
	text-decoration: none;
	color: #fff;
	font-size: 20px;
}
.supporters {
	background: #e6e6e6;
	padding: 20px 20px 1px;
	margin-bottom: 30px;
}
.supporters h3 {
	margin: 0 0 10px;
}


#main {
	margin-bottom: 30px;
}
#left {
	width: 200px;
	margin-right: 30px;
	float: left;
}
#center {
	width: 720px;
	float: left;
}

.block {
	background: #fff;
	padding: 20px 30px;
	margin-bottom: 30px;
}


.menu {
	margin-bottom: 30px;
}
.menu ul {
	list-style-type: none;
	margin: 0;
	padding: 0;
}
.menu ul li {
	margin-bottom: 5px;
}
.menu ul li a {
	text-decoration: none;
	color: #000;
	display: inline-block;
	background: #1d7373;
	padding: 5px 10px;
}
.menu ul li a.active {
	position: relative;
	left: -20px;
}


.policy {
	background: #e6e6e6;
	padding: 10px;
}

footer .info {
	float: left;
	width: 300px;
	padding-bottom: 20px;
	padding-top: 10px;
}
footer .info a {
	text-decoration: none;
	color: #6e6e6e;
}
footer .sign {
	float: right;
	text-align: right;
	width: 300px;
	padding-bottom: 20px;
}
footer .sign a {
	display: inline-block;
	background: #1d7373;
	text-decoration: none;
	color: #000;
	padding: 10px 15px;
}

.socialcount {
	float: right;
	margin: 0 0 10px 10px;
}
